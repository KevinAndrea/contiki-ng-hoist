CFLAGS = -DCONTIKI=1 -DCONTIKI_TARGET_$(TARGET_UPPERCASE)=1 -DCONTIKI_TARGET_STRING=\"$(TARGET)\" -Wno-unused-const-variable -DPROJECT_CONF_PATH=\"project-conf.h\" -g -gstabs+ -mmemory-model=$(TARGET_MEMORY_MODEL) -ffunction-sections -fdata-sections -mcode-region=any -Os -fno-strict-aliasing $(CFLAGSNO) -DMAC_CONF_WITH_CSMA=1 -DNETSTACK_CONF_WITH_IPV6=1 -DROUTING_CONF_RPL_CLASSIC=1 ${addprefix -D,${subst $(COMMA), ,$(DEFINES)}} ${addprefix -I,$(SOURCEDIRS) $(CONTIKI)} -DCONTIKI_VERSION_STRING=\"Contiki-NG-$(RELSTR)\"
